<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>



<table>
    <thead>
        <th>Name</th>
        <th>Email</th>
    </thead>
    <tbody>
<% @users.each do |user| %>
    <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
    </tr>
<% end %>
    </tbody>
</table>


<%= render "pages/home" %>

<%= link_to "New", new_user_path %>
